{
    "manifest_version": 3,
    "name": "Phishing Blocker",
    "version": "1.0",
    "permissions": ["declarativeNetRequest", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "declarative_net_request": {
      "rule_resources": [
        {
          "id": "ruleset_1",
          "enabled": true,
          "path": "./rules.json"
        }
      ]
    }
  }
  